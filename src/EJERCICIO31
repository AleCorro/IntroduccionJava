public class EJERCICIO31 {

    public static void main(String[] args) {

        System.out.println("Los primeros 50 numeros son: ");
        int num = 0;
        for (int i = 0; i < 50;) {
            if (num >= 0) {
                System.out.println(i);
                i++;
            }
            num++;
        }
    }
}
